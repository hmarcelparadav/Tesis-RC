replot[1:500] 'CerenGenElectHadr.dat' u 0:1 w histeps lw 3 t "Hadrones"
replot[1:500] 'CerenGenElectMuon.dat' u 0:1 w histeps lw 3 t "Muones"
replot[1:500] 'CerenGenElectNeut.dat' u 0:1 w histeps lw 3 t "Neutrones"





